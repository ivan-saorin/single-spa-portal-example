{
  "name": "webpack-4-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:dev",
    "build:dev": "webpack-dev-server --env dev --mode none",
    "build:prod": "webpack --env prod --mode production",
    "e2e": "del dist /S /F /Q >> ./dist-cleanup-log.txt && tsc && jest ./dist/src/specs/index.spec.js --driver=firefox",
    "e2e2": "del dist /S /F /Q && tsc && jest ./dist/src/specs/index.spec.js --driver=firefox && jest ./dist/src/specs/index.spec.js --driver=chrome",
    "depcruiser:start": "node ./node_modules/dependency-cruiser/bin/dependency-cruise.js --include-only \"^src\" --output-type dot src | dot -T svg > dependency-graph.svg",
    "depcruiser:start2": "node ./node_modules/dependency-cruiser/bin/dependency-cruise.js --max-depth 3 --output-type dot src | dot -T svg > dependency-graph.svg"
  },
  "author": "mwieth",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/selenium-webdriver": "^4.0.9",
    "autoprefixer": "^8.6.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "chromedriver": "^80.0.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.2.0",
    "dependency-cruiser": "^8.0.1",
    "file-loader": "^5.1.0",
    "geckodriver": "^1.19.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.13.1",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^2.1.5",
    "sass-loader": "^7.0.2",
    "selenium-webdriver": "^4.0.0-alpha.7",
    "style-loader": "^0.21.0",
    "ts-loader": "^6.2.1",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^13.9.0",
    "axios": "^0.19.2",
    "canvas": "^2.5.0",
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2",
    "short-uuid": "^3.1.1"
  }
}
